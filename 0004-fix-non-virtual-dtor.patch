From b79e926d3e197bf306e18e28875afaed1b29b25c Mon Sep 17 00:00:00 2001
From: Tobias Jungel <tobias.jungel@gmail.com>
Date: Wed, 24 Jan 2018 09:08:05 +0100
Subject: [PATCH 4/7] fix non-virtual-dtor

compiling with -Wnon-virtual-dtor works with this patch
---
 include/grpc++/impl/codegen/core_codegen_interface.h | 2 ++
 include/grpc++/impl/codegen/grpc_library.h           | 1 +
 2 files changed, 3 insertions(+)

diff --git a/include/grpc++/impl/codegen/core_codegen_interface.h b/include/grpc++/impl/codegen/core_codegen_interface.h
index d7ad7a4b57..075cc66a78 100644
--- a/include/grpc++/impl/codegen/core_codegen_interface.h
+++ b/include/grpc++/impl/codegen/core_codegen_interface.h
@@ -36,6 +36,8 @@ namespace grpc {
 /// \warning This interface should be considered internal and private.
 class CoreCodegenInterface {
  public:
+  virtual ~CoreCodegenInterface() = default;
+
   /// Upon a failed assertion, log the error.
   virtual void assert_fail(const char* failed_assertion, const char* file,
                            int line) = 0;
diff --git a/include/grpc++/impl/codegen/grpc_library.h b/include/grpc++/impl/codegen/grpc_library.h
index 6036ad7eaa..38b46d77f7 100644
--- a/include/grpc++/impl/codegen/grpc_library.h
+++ b/include/grpc++/impl/codegen/grpc_library.h
@@ -25,6 +25,7 @@ namespace grpc {
 
 class GrpcLibraryInterface {
  public:
+  virtual ~GrpcLibraryInterface() = default;
   virtual void init() = 0;
   virtual void shutdown() = 0;
 };
-- 
